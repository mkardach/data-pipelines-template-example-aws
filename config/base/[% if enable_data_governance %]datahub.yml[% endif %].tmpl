# File generated by a template

source:
  type: "dbt"
  config:
    manifest_path: "./target/manifest.json"
    catalog_path: "./target/catalog.json"
    sources_path: "./target/sources.json"

    target_platform: "[[ execution_environment ]]"
    env: "DEV"

transformers:
  - type: "simple_add_dataset_tags"
    config:
      tag_urns:
        - "urn:li:tag:[[ project_name ]]"
  - type: "simple_add_dataset_ownership"
    config:
      owner_urns:
        - "urn:li:corpuser:[[ pipeline_owner ]]"
      ownership_type: "PRODUCER"
  - type: "set_dataset_browse_path"
    config:
      replace_existing: True
      path_templates:
        - /DATASET_PARTS

sink:
  type: "datahub-rest"
  config:
    server: ${DATAHUB_GMS_URL}
    token: ${DATAHUB_TOKEN}